{
  "manifest_version": 3,
  "name": "YouTube Video ID Fetcher",
  "version": "1.2",
  "description": "Fetches and displays the video ID of the current YouTube video.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
