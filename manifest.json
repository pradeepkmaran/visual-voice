{
    "manifest_version": 3,
    "name": "Audio Player Extension",
    "version": "1.0",
    "description": "An extension that plays audio on button click.",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "audio.html"
    }
  }
  